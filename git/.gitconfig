[push]
  default = current
[pull]
	rebase = false
[core]
  excludesfile = /Users/bfrank/.gitignore
  editor = vim
[alias]
  mylog = 'log --oneline --decorate --graph --all'
  sb = status -sb
  co = checkout
  unstage = 'reset HEAD --'
  history = 'log --full-history --'
[pager]
  diff = diff-so-fancy | less --tabs=1,5 -RFX
  show = diff-so-fancy | less --tabs=1,5 -RFX
[include]
  path = ~/.gitconfig.local
